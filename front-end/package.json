  "homepage": "https://react-bootstrap.github.io/",
  "scripts": {
    "bundle": "webpack --config webpack.config.js",
    "prestart": "npm run bundle",
    "start": "npm install && npm install --save-dev babel-cli && npm run build && npm run docs",
    "build": "babel-node tools/build-cli.js",
    "docs": "babel-node dev/dev-run.js",
    "docs-build": "babel-node tools/build-cli.js --docs-only",
    "docs-prod": "npm install && npm install --save-dev babel-cli && npm run build && npm run docs-build && cross-env NODE_ENV=production babel-node dev/server.js",
    "docs-prod-unoptimized": "sudo npm run docs-build --dev && cross-env NODE_ENV=production babel-node dev/server.js",
    "lint": "eslint src test tools webpack *.js",
    "release": "release",
    "tdd": "karma start",
    "test": "sudo npm run lint && sudo npm run test-browser && sudo npm run test-node",
    "test-browser": "cross-env NODE_ENV=test karma start --single-run",
    "test-node": "mocha --compilers js:babel-register test/server/*Spec.js"
  },
